{
  "schemaVersion": 1,
  "id": "autocomplete",
  "version": "${version}",
  "name": "AutoComplete",
  "description": "Adds an auto complete feature that auto completes words when pressed TAB key.",
  "authors": ["MrSam7K"],
  "contact": {
    "homepage": "https://modrinth.com/mod/autocomplete",
    "issues": "https://github.com/MrSam7K/AutoComplete/issues"
  },
  "license": "MIT",
  "icon": "assets/autocomplete/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.mrsam7k.autocomplete.client.AutoCompleteClient"
    ],
    "main": [
      "me.mrsam7k.autocomplete.AutoComplete"
    ]
  },
  "mixins": [
    "autocomplete.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
